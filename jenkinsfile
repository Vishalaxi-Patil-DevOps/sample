pipeline {
    agent any

    stages {
        stage("Git Checkout") {
            steps {
               git url: 'https://github.com/Vishalaxi-Patil-DevOps/sample.git'
            }
        }
        stage("Build docker image") {
            steps {
                script {
                    sh 'docker build C:\ProgramData\Jenkins\.jenkins\jobs\job -t myappimage'
                }
            }
        }
    }

}