pipeline {
    agent any

    stages {
        stage("Git Checkout") {
            steps {
               git url: 'https://github.com/Vishalaxi-Patil-DevOps/sample.git'
            }
        }
        stage("Build docker image") {
            steps {
                script {
                    bat 'docker build -t myappimage .'
                }
            }
        }
    }

}
